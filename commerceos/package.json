  "name": "@pe/user",
  "version": "4.1.0",
  "description": "Payever CommerseOS Hub",
  "scripts": {
    "env:prepare": "envsubst < deploy/env.json > src/env.json",
    "precommit": "echo 0",
    "cli": "pe-i18n-cli",
    "i18n:dev": "npm run i18n:serve",
    "i18n:dev:temp": "cd ./dist && mkdir -p i18n && npm run i18n-cli flatten assets/locale/app.json dist/i18n/frontend-commerceos-app-en.json && npm run i18n-cli flatten node_modules/@pe/ng-kit/assets/locale/ng-kit.json dist/i18n/frontend-ng-kit-ng-kit-en.json",
    "i18n:serve": "npm run cli ngcli_i18n && I18N_HOST=translation-backend.test.devpayever.com pe-i18n-cli push --domain-prefix=commerceos && I18N_HOST=translation-backend.test.devpayever.com pe-i18n-cli pull --output-path=./i18n-serve --domain-prefix=commerceos",
    "i18n:prod": "rimraf dist/i18n && I18N_HOST=translate.payever.de pe-i18n-cli push --domain-prefix=commerceos && I18N_HOST=translate.payever.de pe-i18n-cli pull --output-path=./dist/i18n --domain-prefix=commerceos",
    "i18n-cli": "pe-i18n-cli",
    "i18n:prepare": "rimraf dist/translations && pe-i18n-cli prepare --domain-prefix=commerceos --prepare-path=dist/translations && pe-i18n-cosf-prepare-connect-app && pe-i18n-cosf-prepare-checkout-app && pe-i18n-cosf-prepare-shop-app && pe-i18n-cosf-prepare-filters-app",
    "start": "ng serve --port=8082 --project=dev --proxy-config proxy.conf.js ",
    "start:telepresence": "npm run env:prepare && npm start --disable-host-check",
    "dev": "npm run start -- --watch",
    "build": "ng build --output-hashing=none --project=main --configuration=prod --deploy-url / && npm run pack && npm run replace-hash && npm run i18n:prepare && npm run build:widgets",
    "test": "rimraf coverage && npm run test:headless",
    "test:dev": "ng test --project=dev --code-coverage --source-map --watch=true --source-map=false --browsers=Chrome",
    "test:headless": "ng test --watch=false",
    "lint": "ng lint",
    "bundle:stats": "ng build --project=main --configuration=prod --stats-json",
    "bundle:report": "webpack-bundle-analyzer dist/stats.json",
    "replace-hash": "pe-replace-hash",
    "pack": "pe-micro-packager",
    "build:widgets": "cd ./node_modules/@pe/finexp-app && cp -a widgets/. ../../../dist"
  },
